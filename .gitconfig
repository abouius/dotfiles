[color]
	ui = true
[push]
	default = current
[branch]
	autosetuprebase = always
[rerere]
	enabled = 1
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = 120
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = 120 bold
[color "status"]
  added = yellow
  changed = 120
  untracked = cyan
